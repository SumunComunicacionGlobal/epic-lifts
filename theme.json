{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"link": false,
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#012F6C",
					"name": "Text main",
					"slug": "foreground"
				},
				{
					"color": "#64748B",
					"name": "Text medium",
					"slug": "text-medium"
				},
				{
					"color": "#00B2FF",
					"name": "Primary 10",
					"slug": "primary-10"
				},
				{
					"color": "#78B6FF",
					"name": "Primary 30",
					"slug": "primary-30"
				},
				{
					"color": "#0064E2",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#001AE2",
					"name": "Primary 80",
					"slug": "primary-80"
				},
				{
					"color": "#1A1734",
					"name": "Primary 100",
					"slug": "primary-100"
				},
				{
					"color": "#F96771",
					"name": "Accent 10",
					"slug": "accent-10"
				},
				{
					"color": "#F73946",
					"name": "Accent 30",
					"slug": "accent-30"
				},
				{
					"color": "#FF5122",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#D00B19",
					"name": "Accent 80",
					"slug": "accent-80"
				},
				{
					"color": "#AE0915",
					"name": "Accent 100",
					"slug": "accent-100"
				},
				{
					"color": "#F5F5F5",
					"name": "Neutral 10",
					"slug": "neutral-10"
				},
				{
					"color": "#E2E8F0",
					"name": "Neutral 30",
					"slug": "neutral-30"
				},
				{
					"color": "#CBD5E1",
					"name": "Neutral 60",
					"slug": "neutral-60"
				},
				{
					"color": "#94A3B8",
					"name": "Neutral 80",
					"slug": "neutral-80"
				},
				{
					"color": "#334155",
					"name": "Neutral 100",
					"slug": "neutral-100"
				}
			]
		},
		"custom": {
			"border-radius": {
				"lg": "16px",
				"md": "8px",
				"rounded": "999px",
				"sm": "4px",
				"xl": "32px",
				"zero": "0"
			},
			"border-width": {
				"large": "4px",
				"medium": "3px",
				"small": "2px",
				"tiny": "1px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link": "var(--wp--preset--color--primary)",
				"neutral-black": "#000000",
				"neutral-white": "#FFFFFF"
			},
			"font-weight": {
				"bold": "600",
				"light": "300",
				"normal": "400"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "40px",
				"content": "1216px",
				"padding": "16px",
				"page": "var(--wp--custom--layout--content)",
				"sidebar": "336px",
				"wide": "90%"
			},
			"letter-spacing": {
				"loose": ".05em",
				"looser": ".1em",
				"none": "normal",
				"tight": "-.01em"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
					"slug": "20"
				},
				{
					"name": "Space 3 (2rem - 32px)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "Space 4 (2.5rem - 40px)",
					"size": "2.5rem",
					"slug": "40"
				},
				{
					"name": "Space 6 (4rem - 64px)",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "Space 7 (4.5rem - 72px)",
					"size": "4.5rem",
					"slug": "70"
				},
				{
					"name": "Space 8 (5rem - 80px)",
					"size": "5rem",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 600",
							"src": [
								"file:./assets/fonts/OpenSans-VariableFont_wdth,wght.ttf"
							]
						},{
							"fontDisplay": "block",
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400 600",
							"src": [
								"file:./assets/fonts/OpenSans-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],						
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 4rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(2.5rem, 4.8vw, 3.5rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 2.25rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "1.875rem",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "clamp(1rem, 4.4vw, 1.4rem)",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "1.25rem",
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "1.125",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "1.25rem",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "1rem",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			],
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/group": {
				"spacing": []
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--rounded)",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": "1rem 2rem"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "var(--wp--preset--font-weight--normal)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontWeight": "var(--wp--custom--font-weight--light)",
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"bottom": "0px",
				"left": "2rem",
				"right": "2rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
		}
	},
	"version": 3
}